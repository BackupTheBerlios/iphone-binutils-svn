AS = arm-apple-darwin-cc ;
ASFLAGS = -c ;
CC = arm-apple-darwin-cc ;
OPTIM = -O0 ;
LINK = ../cctools/ld/ld_dir/ld.NEW  ;
LINKFLAGS = -r -keep_private_externs ;

LOCATE_TARGET = dynamic-no-pic ;
CCFLAGS = -Wall -DDYNAMIC_NO_PIC -relocation-model=dynamic-no-pic
    -DMACH_HEADER_SYMBOL_NAME=__mh_execute_header ;
Objects arm-start.s arm-dyld-glue.s crt.c ;
MainFromObjects crt1.o : arm-start.o arm-dyld-glue.o crt.o dyld ;

LOCATE_TARGET = pic ;
CCFLAGS = -Wall -DPIC -D__PIC__ -relocation-model=pic
    -DMACH_HEADER_SYMBOL_NAME=__mh_dylib_header ;
Objects arm-start.s arm-dyld-glue.s crt.c ;
MainFromObjects dylib.o : arm-start.o arm-dyld-glue.o crt.o dyld ;

